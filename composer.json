{
    "name": "vasileuski/magento2-module-admin-search",
    "type": "magento2-module",
    "version": "1.2.1",
    "description": "Magento 2 module that enhances the admin panel's global search by enabling fast and efficient searches through main entities with improved visibility and ACL support.",
    "keywords": ["magento", "magento2", "admin", "search"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Dzmitry Vasileuski",
            "email": "vasileuski_dzmitry@outlook.com"
        }
    ],
    "require": {
        "php": "~8.1.0||~8.2.0||~8.3.0",
        "ext-intl": "*",
        "magento/module-catalog": "104.0.*",
        "magento/module-catalog-rule": "101.2.*",
        "magento/module-cms": "104.0.*",
        "magento/module-customer": "103.0.*",
        "magento/module-elasticsearch": "101.0.*",
        "magento/module-sales": "103.0.*",
        "magento/module-sales-rule": "101.2.*",
        "magento/module-store": "101.1.*",
        "magento/framework": "103.0.*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Vasileuski\\AdminSearch\\": ""
        }
    }
}
